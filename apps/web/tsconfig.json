{
	"extends": "@repo/typescript-config/nextjs.json",
	"compilerOptions": {
		"types": ["@serwist/next/typings"],
		"lib": ["webworker", "DOM"],
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": ["next-env.d.ts", "next.config.mjs", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
	"exclude": ["node_modules"]
}
